   #include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int i,j;
    int a[1000],num;
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    scanf("%d",&num);
    
    int sum=0;
    for(i=0 ; i<num ; i++)
    {
        scanf("%d", &a[i]);
        sum=sum+a[i];
    }

    printf("%d",sum);



    return 0;
}
