  
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n,a,rem,sum=0;
    scanf("%d", &n);
    //Complete the code to calculate the sum of the five digits on n.
    for(a=n;a>0;a/=10)
    {
        rem=a%10;
        sum=rem+sum;
    }
    printf("%d",sum);
    return 0;
}
